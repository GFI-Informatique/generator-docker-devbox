{
  "compilerOptions": {
    "target": "es2016",
    "module":"commonjs",
    "lib": ["es2016"],
    "strict": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "outDir": "generators",
    "rootDir": "generators",
    "typeRoots": [
      "typings",
      "node_modules/@types"
    ]
  },
  "include": [
    "generators/**/*.ts"
  ],
  "exclude": [
    "**/templates"
  ]
}
