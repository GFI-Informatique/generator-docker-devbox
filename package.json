{
  "name": "@gfi-centre-ouest/generator-docker-devbox",
  "version": "1.1.0",
  "description": "Yeoman Generator for dockerized development environment",
  "homepage": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "author": {
    "name": "GFI Orléans - Pôle Digital",
    "email": "pole-digital-centre@gfi.fr",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "npm run tslint && npm run shlint",
    "shlint": "shellcheck ./generators/app/templates/.bash_enter ./generators/app/templates/.bash_leave ./generators/app/templates/.bash_enter.d/* ./generators/app/templates/.bash_leave.d/*",
    "tslint": "tslint --project tsconfig.json -t codeFrame 'generators/**/*.ts' '__tests__/**/*.ts' '__tests__/**/*.ts'",
    "clean": "del-cli 'generators/**/*.js' 'generators/**/*.js.map' '!generators/**/templates/**/*'",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run clean",
    "start": "tsc -w",
    "test": "npm run build && jest --coverage --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --coverage --watch",
    "test:prod": "jest --coverage --verbose --forceExit --no-cache",
    "test:docker": "jest --config jest.docker.config.js --runInBand --verbose --forceExit --no-cache",
    "release": "npm run lint && npm run build && npm run test && np"
  },
  "keywords": [
    "docker-devbox",
    "docker",
    "docker-compose",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@gfi-centre-ouest/docker-compose-spec-typescript": "^1.0.0",
    "@types/deep-extend": "^0.4.31",
    "@types/ejs": "^2.6.0",
    "@types/gulp-rename": "0.0.33",
    "@types/handlebars": "^4.0.40",
    "@types/handlebars-helpers": "^0.5.0",
    "@types/inquirer": "0.0.43",
    "@types/jest": "^23.3.9",
    "@types/js-yaml": "^3.11.2",
    "@types/lodash": "^4.14.118",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10.12.3",
    "@types/semver": "^5.5.0",
    "@types/shelljs": "^0.8.0",
    "@types/vinyl": "^2.0.2",
    "@types/yeoman-generator": "^3.0.1",
    "@types/yeoman-test": "^1.7.4",
    "@types/yosay": "0.0.29",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0",
    "jest-circus": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "np": "*",
    "rimraf": "^2.6.2",
    "shellcheck": "^0.1.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0",
    "yo": "^2.0.5"
  },
  "dependencies": {
    "@gfi-centre-ouest/docker-compose-builder": "^0.7.0",
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "handlebars": "<4.1.0",
    "handlebars-helpers": "^0.10.0",
    "js-yaml": "^3.12.0",
    "left-pad": "^1.3.0",
    "mustache": "^3.0.1",
    "retry-axios": "^0.3.2",
    "semver": "^5.6.0",
    "shelljs": "^0.8.2",
    "source-map-support": "^0.5.9",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "license": "MIT"
}
