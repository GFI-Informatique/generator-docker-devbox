{
  "name": "@gfi-centre-ouest/generator-docker-devbox",
  "version": "0.14.0",
  "description": "Yeoman Generator for dockerized development environment",
  "homepage": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "author": {
    "name": "GFI Orléans - Pôle Digital",
    "email": "pole-digital-centre@gfi.fr",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json -t codeFrame 'generators/**/*.ts' '__tests__/**/*.ts'",
    "clean": "del-cli 'generators/**/*.js' 'generators/**/*.js.map' '!generators/**/templates/**/*'",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run clean",
    "start": "tsc -w",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "release": "npm run lint && npm run build && npm run test && np"
  },
  "keywords": [
    "docker-devbox",
    "docker",
    "docker-compose",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/deep-extend": "^0.4.31",
    "@types/gulp-rename": "0.0.33",
    "@types/handlebars": "^4.0.39",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.3",
    "@types/shelljs": "^0.8.0",
    "@types/yeoman-generator": "^3.0.0",
    "@types/yosay": "0.0.29",
    "del-cli": "^1.1.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lodash": "^4.17.11",
    "np": "*",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0",
    "yo": "^2.0.5"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "glob": "^7.1.3",
    "gulp-rename": "<1.2.3",
    "handlebars": "^4.0.12",
    "handlebars-helpers": "^0.10.0",
    "shelljs": "^0.8.2",
    "source-map-support": "^0.5.9",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "license": "MIT"
}
