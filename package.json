{
  "name": "generator-docker-devbox",
  "version": "0.1.0",
  "description": "Générateur Yeoman pour les environnements dockerisés via docker-devbox",
  "homepage": "https://glbaso01.asogfi.fr/PoleDigital/generator-docker-devbox",
  "author": {
    "name": "GFI Orléans - Pôle Digital",
    "email": "pole-digital-centre@gfi.fr",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "docker-devbox",
    "docker",
    "docker-compose",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "yo": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "glob": "^7.1.2",
    "gulp-rename": "^1.2.2",
    "handlebars": "^4.0.11",
    "shelljs": "^0.7.8",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://glbaso01.asogfi.fr/PoleDigital/generator-docker-devbox.git",
  "license": "MIT"
