{
  "name": "@gfi-centre-ouest/generator-docker-devbox",
  "version": "1.8.2",
  "description": "Yeoman Generator for dockerized development environment",
  "homepage": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "author": {
    "name": "GFI Orléans - Pôle Digital",
    "email": "pole-digital-centre@gfi.fr",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "scripts": {
    "lint": "npm run tslint",
    "tslint": "tslint --project tsconfig.json -t codeFrame 'generators/**/*.ts' '__tests__/**/*.ts' '__tests__/**/*.ts'",
    "clean": "del-cli 'generators/**/*.js' 'generators/**/*.js.map' '!generators/**/templates/**/*'",
    "prebuild": "npm run clean",
    "build": "tsc",
    "prestart": "npm run clean",
    "start": "tsc -w",
    "test": "npm run build && jest --coverage --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --coverage --watch",
    "test:prod": "jest --coverage --verbose --forceExit --no-cache",
    "test:docker": "jest --config jest.docker.config.js --runInBand --verbose --forceExit --no-cache",
    "release": "npm run lint && npm run build && npm run test && np"
  },
  "keywords": [
    "docker-devbox",
    "docker",
    "docker-compose",
    "yeoman",
    "generator",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@gfi-centre-ouest/docker-compose-spec-typescript": "^1.0.0",
    "@types/deep-extend": "^0.4.31",
    "@types/ejs": "^2.6.3",
    "@types/gulp-rename": "0.0.33",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.19",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.144",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.11.1",
    "@types/semver": "^6.0.2",
    "@types/shelljs": "^0.8.5",
    "@types/vinyl": "^2.0.3",
    "@types/yeoman-generator": "^3.1.4",
    "@types/yeoman-test": "^2.0.3",
    "@types/yosay": "0.0.29",
    "del-cli": "^3.0.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "jest-cli": "^24.9.0",
    "lodash": "^4.17.15",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.0.0",
    "yo": "^3.1.0"
  },
  "dependencies": {
    "@gfi-centre-ouest/docker-compose-builder": "^0.8.0",
    "@octokit/rest": "^16.33.1",
    "@types/handlebars-helpers": "^0.5.2",
    "@types/mem-fs": "^1.1.2",
    "@types/mem-fs-editor": "^5.1.1",
    "@types/yeoman-assert": "^3.1.1",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "glob": "^7.1.4",
    "handlebars": "^4.4.3",
    "handlebars-helpers": "^0.10.0",
    "js-yaml": "^3.13.1",
    "left-pad": "^1.3.0",
    "mustache": "^3.1.0",
    "np": "^5.1.1",
    "retry-axios": "^1.0.1",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.13",
    "yeoman-generator": "^4.1.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": "https://github.com/gfi-centre-ouest/generator-docker-devbox",
  "license": "MIT"
}
